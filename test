#/bin/bash

clear

echo 'Please enter the id of the round: '
read round
echo 'Please enter the name of the problem: '
read problem

g++ -std=c++11 $problem.cpp

wget http://codeforces.com/contest/$round/problem/$problem

clear
mv $problem .nameIn

echo ' '
echo ' '
./.getInput
./.runTests
echo ' '
echo ' '
echo ' '


